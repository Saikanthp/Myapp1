pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh '''echo "compiling java project..."
'''
      }
    }

    stage('Test') {
      steps {
        echo 'Running tests...'
        sh '''echo "Running test"

'''
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploying application...'
        sh '''echo"deploying the application"
'''
      }
    }

  }
}